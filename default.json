{
  "extends": [
    "config:recommended",
    ":separateMultipleMajorReleases",
    ":timezone(Asia/Tokyo)",
    "github>walnuts1018/renovate-config:assign",
    "github>walnuts1018/renovate-config:automergePin",
    "github>walnuts1018/renovate-config:automergeTypesMinor",
    "github>walnuts1018/renovate-config:groupAndroidPackages",
    "github>walnuts1018/renovate-config:groupCocoaPodsPackages",
    "github>walnuts1018/renovate-config:groupGolangPackages",
    "github>walnuts1018/renovate-config:groupJest",
    "github>walnuts1018/renovate-config:groupLinters",
    "github>walnuts1018/renovate-config:limitDockerNodeUpdatesToLTS",
    "github>walnuts1018/renovate-config:postUpdateOptions",
    "github>walnuts1018/renovate-config:vulnerabilityAlerts",
    "github>walnuts1018/renovate-config:pinGitHubActionDigests"
  ],
  "labels": [
    "renovate"
  ],
  "dockerfile": {
    "fileMatch": [
      "(^|/)Dockerfile-[^/]*$"
    ]
  },
  "npm": {
    "rangeStrategy": "bump"
  },
  "packageRules": [
    {
      "groupName": "boto3",
      "matchPackageNames": [
        "boto3",
        "botocore"
      ]
    }
  ]
}
